{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",         
            "options": {
                "cwd": "${fileDirname}",
                "env": {
                    "PHYSXPATH": "common/PhysX-3.4"
                }
            },
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/Fe${fileDirname}\\${fileBasenameNoExtension}.exe",
                "${file}",
                "--include-directory=${PHYSXPATH}/PhysX_3.4/Include;${PHYSXPATH}/PxShared/include;${PHYSXPATH}/PxShared/src/foundation/include;${PHYSXPATH}/PxShared/src/fastxml/include;../common/Graphics/include/win32/GL;../common/PhysX-3.4/PhysX_3.4/Include;../common/PhysX-3.4/PxShared/include;../common/PhysX-3.4/PxShared/src/foundation/include;../common/PhysX-3.4/PxShared/src/fastxml/include;%(AdditionalIncludeDirectories)"
            ],
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}